{
	"name": "chat-monitor",
	"version": "v9.0.0-Nirbose",
	"author": "__fallenreaper",
	"description": "throttles ability for users to post",
	"main": "dist/index.js",
	"type": "module",
	"style": "options",
	"imports": {
		"#config": ["./src/config.ts", "./dist/config.js"],
		"#types": ["./src/types.ts", "./dist/types.js"],
		"#core/*": ["./src/core/*.ts", "./dist/core/*.js"],
		"#tables/*": ["./src/tables/*.ts", "./dist/tables/*.js"],
		"#buttons/*": ["./src/buttons/*.ts", "./dist/buttons/*.js"],
		"#namespaces/*": ["./src/namespaces/*.ts", "./dist/namespaces/*.js"],
		"#all": ["./src/core/index.ts", "./dist/core/index.js"]
	},
	"scripts": {
		"bot": "npx @ghom/bot.ts-cli",
		"build": "rimraf dist && npx rollup -c && node scripts/copy-keepers.js",
		"final": "rimraf node_modules && npm install --production && npm run build",
		"watch": "echo \"No watch support for this runtime\" && exit 1",
		"start": "npm run build && node dist/index.js",
		"start.test": "npm run build && node dist/index.test.js",
		"format": "npx biome check --write src scripts package.json",
		"update": "node scripts/update-framework.js",
		"readme": "npm run build && node scripts/generate-readme.js",
		"test": "npm run format && tsc --noEmit && npm run start.test"
	},
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bot-ts/framework.git"
	},
	"bugs": {
		"url": "https://github.com/bot-ts/framework/issues"
	},
	"homepage": "https://github.com/bot-ts/framework#readme",
	"dependencies": {
		"@ghom/handler": "^3.1.0",
		"@ghom/logger": "^2.0.2",
		"@ghom/orm": "^1.9.1",
		"dayjs": "^1.11.13",
		"discord-eval.ts": "1.1.3",
		"discord.js": "^14.21.0",
		"dotenv": "^16.6.1",
		"ejs": "^3.1.10",
		"ghom-prettify": "^3.0.0",
		"install": "^0.13.0",
		"node-cron": "^3.0.3",
		"regex-parser": "^2.3.1",
		"rimraf": "^6.0.1",
		"simple-git": "^3.28.0",
		"sqlite3": "latest",
		"tims": "^2.1.0",
		"types-package-json": "^2.0.39",
		"yargs-parser": "^21.1.1",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@ghom/bot.ts-cli": "9.0.14",
		"@rollup/plugin-alias": "^5.1.1",
		"@rollup/plugin-node-resolve": "^15.3.1",
		"@rollup/plugin-typescript": "^12.1.4",
		"@types/bun": "^1.2.20",
		"@types/deno": "^2.3.0",
		"@types/node": "^22.17.2",
		"@types/node-cron": "^3.0.11",
		"@types/rollup": "^0.51.4",
		"@types/yargs-parser": "^21.0.3",
		"discord-api-types": "^0.37.120",
		"fast-glob": "3.3.2",
		"git-commit-info": "2.0.2",
		"rollup": "^4.46.2",
		"typescript": "^5.9.2"
	},
	"optionalDependencies": {
		"@types/pg": "^8.15.5"
	},
	"engines": {
		"node": ">=22.x.x",
		"npm": ">=10.x.x",
		"yarn": ">=1.22.22"
	}
}
